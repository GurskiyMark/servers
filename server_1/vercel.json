{
  "version": 2,
  "builds": [
    { "src": "src/index.ts", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/blogs", "dest": "src/index.ts" },
    { "src": "/blogs/(.*)", "dest": "src/index.ts" },
    { "src": "/posts", "dest": "src/index.ts" },
    { "src": "/posts/(.*)", "dest": "src/index.ts" },
    { "src": "/testing/all-data", "dest": "src/index.ts" }
  ]
}
